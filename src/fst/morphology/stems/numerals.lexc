          kuttlov:kutt#lov   LOGIT ;
        čiččâmlov:čiččâm#lov LOGIT ;
         kähcilov:kähci#lov  LOGIT ;
          ohcelov:ohce#lov   LOGIT ;

LEXICON LOGIT !!≈ * **@CODE@**
 ohtâ:oht^RVâ THOUSANDS ;
              ONEST     ;
              THOUSANDS ;

LEXICON ONEST !!≈ * **@CODE@**
 kyeht THOUSANDS ; ! 2000-2999
 kulmâ THOUSANDS ; ! etc.
 nelji THOUSANDS ;
 vittâ THOUSANDS ;
 kuttâ THOUSANDS ;
čiččâm THOUSANDS ;
käävci THOUSANDS ;
 oovce THOUSANDS ;

LEXICON THOUSANDS !!≈ * **@CODE@**
 tuhháát THOUSAND ; ! 1000-1999


LEXICON THOUSAND !!≈ * **@CODE@**
       UNDERTHOUSAND ; ! 1100-1999
       TENS          ; ! 1020-1099
       TEENS         ; ! 1010-1019
       ONES          ; ! 1001-1009
 +Num: X_POTTAAK     ; ! 1000



