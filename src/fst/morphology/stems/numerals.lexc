!! # Inari Saami numerals

!! ## Main lexicon

LEXICON Numeral !!≈ * **@CODE@**

         HUNDREDSM       ; ! 200M
   čyeti HUNDREDM        ; ! 100M
         TENSM           ; ! 20-99M
         TEENSM          ; ! 10-19M
         ONESM           ; ! 1-9M
         HUNDREDST       ; ! 200000-999999
         HUNDREDST       ; ! 200000-999999
   čyeti HUNDREDT        ; ! 100000-100999
         TENST           ; ! 20000-99999,10000-10999
         TEENST          ; ! 11000-19999
         ONEST           ; ! 2000-9999
 tuhháát THOUSAND        ; ! 1000-1999
         UNDERTHOUSAND   ; ! 100-999
         TENS            ; ! 20-99
         TEENS           ; ! 10-19
         ONES            ; ! 1-9
         ARABIC-COLL     ;

         num-imprecise   ; ! kalle, ...
         MISCNUM         ; ! pelnub
         ARABICS         ; ! for the arabic numerals !
         ROMAN           ; ! for the roman numerals  !
         NUM-PREFIXES    ; ! for §34 etc.            !
         ISOLATED-NUMEXP ; ! for ½ etc.                      !
         LOHKAI          ;

!! ## The million layer

LEXICON HUNDREDSM   !!≈ * **@CODE@**
 kyehti CUODIM ;
  kulmâ CUODIM ;
  nelji CUODIM ;
  vittâ CUODIM ;
  kuttâ CUODIM ;
 čiččâm CUODIM ;
 käävci CUODIM ;
  oovce CUODIM ;


LEXICON CUODIM  !!≈ * **@CODE@**
 čyeti HUNDREDM ;

LEXICON HUNDREDM  !!≈ * **@CODE@**
  TENSM  ;
  TEENSM ;
  ONESM  ;
  MILJON ;

LEXICON TEENSM  !!≈ * **@CODE@**
  TEENM ;

LEXICON TEENM  !!≈ * **@CODE@**
         ohtâ:oht^RVâ LOHKAIM ;
       kyehti         LOHKAIM ;
        kulmâ         LOHKAIM ;
        nelji         LOHKAIM ;
 vitt+Use/NG:vittâ   LOHKAIM ;
 kutt+Use/NG:kuttâ   LOHKAIM ;
         vitt         LOHKAIM ;
         kutt         LOHKAIM ;
       čiččâm         LOHKAIM ;
       käävci         LOHKAIM ;
        oovce         LOHKAIM ;

LEXICON LOHKAIM  !!≈ * **@CODE@**
 nubáloh MILJON ;

LEXICON TENSM  !!≈ * **@CODE@**
   love MILJON ;
 kyehti LOGIM  ;
  kulmâ LOGIM  ;
  nelji LOGIM  ;
  vittâ LOGIM  ;
  kuttâ LOGIM  ;
 čiččâm LOGIM  ;
 käävci LOGIM  ;
  oovce LOGIM  ;

LEXICON LOGIM  !!≈ * **@CODE@**
 love MILJON ;
 love ONESM  ;

LEXICON ONESM            !!≈ * **@CODE@**
 miljon%  OVERTHOUSANDS ;
   kyehti MILJON        ;
    kulmâ MILJON        ;
    nelji MILJON        ;
    vittâ MILJON        ;
    kuttâ MILJON        ;
   čiččâm MILJON        ;
   käävci MILJON        ;
    oovce MILJON        ;

LEXICON MILJON  !!≈ * **@CODE@**
 % miljona%   OVERTHOUSANDS ;

! =================
!! ## The under the million
! =================

!! In principle as the above.

LEXICON OVERTHOUSANDS !!≈ * **@CODE@**
         HUNDREDST     ; ! x00.000.000
   čyeđe HUNDREDT      ; ! 100.000.000
         TENST         ; !  20.000.000
         TEENST        ; !  10.000.000
 tuhháát THOUSAND      ; !   1.000.000
         UNDERTHOUSAND ;
         TENS          ;
         TEENS         ;
         ONES          ;
         num           ; !  x000000


LEXICON HUNDREDST !!≈ * **@CODE@**
          kyeht        CYEDET   ; ! 200000-299999
 kyeht+Err/Orth:kyehti CYEDET   ; ! 200000-299999
          kulmâ        CYEDET   ; ! 300000-399999
          nelji        CYEDET   ; ! 400000-499999
           vitt        CYEDET   ; ! 500000-599999
           kutt        CYEDET   ; ! 600000-699999
  vitt+Err/Orth:vit    CYEDET   ; ! 500000-599999
  kutt+Err/Orth:kut    CYEDET   ; ! 600000-699999
  vitt+Err/Orth:vittâ  CYEDET   ; ! 500000-599999
  kutt+Err/Orth:kuttâ  CYEDET   ; ! 600000-699999
         čiččâm        CYEDET ; ! 700000-799999
         käävci        CYEDET ; ! 800000-899999
          oovce        CYEDET ; ! 900000-999999

LEXICON CYEDET !!≈ * **@CODE@**
 čyeđe HUNDREDT ; ! 100000-199999


LEXICON HUNDREDT	 !!≈ * **@CODE@**  X = 1-9, Y = 0-9
              TENST        ; ! X2XYYY, X10YYY
              TEENST       ; ! X1XYYY
 ohtâ:oht^RVâ THOUSANDS ;    ! Same tuhháát as 2000
              THOUSANDS    ; ! X00YYY
              ONEST        ; ! XX0YYY

LEXICON TEENST !!≈ * **@CODE@**
  TEENT ;

LEXICON TEENT !!≈ * **@CODE@**
         love         THOUSANDS ;
         ohtâ:oht^RVâ LOHKAIT   ;
       kyehti         LOHKAIT   ;
        kulmâ         LOHKAIT   ;
        nelji         LOHKAIT   ;
         vitt         LOHKAIT   ;
 vittâ+Use/NG:vittâ   LOHKAIT   ;
         kutt         LOHKAIT   ;
 kuttâ+Use/NG:kuttâ   LOHKAIT   ;
       čiččâm         LOHKAIT   ;
       käävci         LOHKAIT   ;
        oovce         LOHKAIT   ;

LEXICON LOHKAIT !!≈ * **@CODE@**
 nubáloh THOUSANDS ;

LEXICON TENST !!≈ * **@CODE@**
          muádlov:muád#lov   LOGIT ;
         maŋgâlov:maŋgâ#lov  LOGIT ;
