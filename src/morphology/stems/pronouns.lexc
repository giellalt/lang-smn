sun+Pron+Pers+Pl3:si perspronpl ;


LEXICON perspronsg !!= * __@CODE@__
+Nom:n	G ;
+Gen:u	      G ;
+Acc:u	      G ;
+Ill:njin	      G ;
+Loc:st	          G ;
+Loc+Use/NG:ste	      G ;
+Com:in	          G ;
+Com+Use/NG:uin	      G ;
+Abe:uttáá	      G ;
+Ess:nen	      G ;
+Par:đe	      G ;

LEXICON persprondu !!= * __@CODE@__
