LEXICON NAAL !!= * __@CODE@__ 
:náál ADV ; 
:náálá ADV ; 


LEXICON BEL-ADV !!= * __@CODE@__ 
bel:#bel ADV ; 
bel+Err/Orth:#pel ADV ; 
beln:#beln ADV ; 
beln+Err/Orth:#peln ADV ; 
bele:#bele ADV ; 
bele+Err/Orth:#pele ADV ; 

LEXICON PEL-ADV !!= * __@CODE@__ 
pel+Err/Orth:#bel ADV ; 
pel:#pel ADV ; 
peln+Err/Orth:#beln ADV ; 
peln:#peln ADV ; 
pele+Err/Orth:#bele ADV ; 
pele:#pele ADV ; 


